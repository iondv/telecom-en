<%
let result = {
  title: item.get('name')
}

if (item.get('chislennost')) {
  result.title += ' (' + item.property('chislennost').evaluate() + ' ppl.)';
}

let itemSvyaz = item.property('svyaz').evaluate();
if (itemSvyaz) {
%>

<ul class='balloon-attrlist' style='width:225px'>
  <li><%- result.title %></li>
</ul>
<ul class='mt10'>
    <%
      let coll = itemSvyaz.property('radio').evaluate();
    if (coll && coll.length) {
      for (let elem of coll) {
      let collR = elem.property('radioCanals').evaluate();
      if (collR && collR.length) {
        for (let rd of collR) {
      let nm = rd.property('name').evaluate();
      let nb = rd.property('numb').evaluate();
      if (nb === null) {
        nb = '';
      } else {
        nb = ' (' + nb + ' MHz)';
      }
      if (rd.get('name') && nm !== null) {
        %>
        <li>
            <%- nm + nb %>
          </li>
  <% } } } } } } %>
</ul>